{
    "name": "idci/keycloak-security-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily handle you application security thanks to keycloak.",
    "license": [
        "GPL-3.0-or-later",
        "CECILL-2.0"
    ],
    "keywords": [],
    "authors": [
        {
            "name": "Benjamin Boudier",
            "email": "benjamin.boudier@idci-consulting.fr"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/dependency-injection": "^3.3 || ^4.0 || ^5.2",
        "symfony/framework-bundle": "^3.0 || ^4.0 || ^5.2",
        "symfony/routing": "^2.7|^3.0|^4.0|^5.2",
        "symfony/http-foundation": "^2.7|^3.0|^4.0|^5.2",
        "knpuniversity/oauth2-client-bundle": "^2.7",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/expression-language": "^3.0 || ^4.0 || ^5.2",
        "symfony/finder": "^3.0 || ^4.0 || ^5.2",
        "symfony/form": "^3.0 || ^4.0 || ^5.2",
        "symfony/stopwatch": "^3.0 || ^4.0 || ^5.2",
        "symfony/twig-bundle": "*",
        "symfony/validator": "^3.0 || ^4.0 || ^5.2",
        "symfony/yaml": "^3.0 || ^4.0 || ^5.2"
    },
    "autoload": {
        "psr-4": {
            "IDCI\\Bundle\\KeycloakSecurityBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IDCI\\Bundle\\KeycloakSecurityBundle\\Tests\\": "Tests"
        }
    }
}
